{
  "title": "自作キーマップ",
  "rules": [
    {
      "description": "ctrl + d => delete",
      "manipulators": [
        {
         "type": "basic",
         "from": {
           "key_code": "b",
           "modifiers": {
             "mandatory": ["control"]
           }
         },
         "to": [
           {
             "key_code": "delete_or_backspace"
           }
         ]
        }
      ]
     },
    {
      "description": "ctrl + ; => return",
      "manipulators": [
        {
         "type": "basic",
         "from": {
           "key_code": "b",
           "modifiers": {
             "mandatory": ["control"]
           }
         },
         "to": [
           {
             "key_code": "return"
           }
         ]
        }
      ]
     }
  ]
}
